{
  "name": "inventory_module",
  "version": "1.0.0",
  "main": "build/index.js",
  "scripts": {
    "start": "node build/index.js",
    "dev": "nodemon src/index.ts | pino-pretty",
    "build": "tsc",
    "test": "jest --watchAll --coverage",
    "typeorm": "typeorm-ts-node-esm",
    "migration:create": "npm run typeorm migration:create ./migrations/history/$npm_config_name",
    "migration:run": "npm run typeorm migration:run -- -d ./migrations/data-source.ts",
    "migration:revert": "npm run typeorm migration:revert -- -d ./migrations/data-source.ts",
    "migration:drop": "npm run typeorm schema:drop -- -d ./migrations/data-source.ts",
    "migration:generate": "npm run typeorm migration:generate -- -d ./migrations/data-source.ts ./migrations/post-refactoring/latest-changes-migration"
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@types/cors": "^2.8.13",
    "axios": "^1.3.5",
    "config": "^3.3.9",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.17.1",
    "pg": "^8.8.0",
    "pino": "^8.11.0",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.30.0",
    "sequelize-typescript": "^2.1.5",
    "ts-node": "^10.9.1",
    "typeorm": "^0.3.11",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@types/config": "^3.3.0",
    "@types/express": "^4.17.13",
    "@types/yamljs": "^0.2.31",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "jest": "^29.5.0",
    "nodemon": "^2.0.13",
    "pino-pretty": "^10.0.0",
    "prettier": "2.8.7",
    "ts-jest": "^29.1.0",
    "typescript": "^4.5.4"
  }
}
